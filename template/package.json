{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "scripts": {
    "build": "webpack --progress --hide-modules",
    "build:android": "npm run build -- --env.android",
    "build:ios": "npm run build -- --env.ios",
    "dev": "webpack --watch --hide-modules",
    "dev:android": "npm run dev -- --env.android",
    "dev:ios": "npm run dev -- --env.ios",
    "postinstall": "tns create dist --template nativescript-vue-base-template --appid {{ app_id }}"
  },
  "dependencies": {
    "nativescript-vue": "1.0.0",
    "tns-core-modules": "3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "nativescript-vue-loader": "^0.1.5",
    "nativescript-vue-target": "^0.1.0",
    "nativescript-vue-template-compiler": "^1.2.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0"
  },
  "author": "{{ author }}",
  "license": "{{ license }}"
}
